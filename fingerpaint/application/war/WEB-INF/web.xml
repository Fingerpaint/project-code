<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
              http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee">

  <!-- Servlets -->
  
  <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>Fingerpaint.html</welcome-file>
  </welcome-file-list>
  
  <servlet>
  <servlet-name>simulatorSeviceImpl</servlet-name>
  <servlet-class>nl.tue.fingerpaint.server.simulator.SimulatorSeviceImpl</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>simulatorSeviceImpl</servlet-name>
    <url-pattern>/fingerpaint/simulatorService</url-pattern>
  </servlet-mapping>
  

</web-app>
